{
  "version": 2,
  "build": {
    "env": {
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.ariflawfirm.com/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://api.ariflawfirm.com/api/:path*/"
    }
  ]
}
